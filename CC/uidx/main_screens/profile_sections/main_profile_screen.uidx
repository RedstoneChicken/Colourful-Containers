  "skin_panel_template": {
    "type": "panel",
    "anchor_from": "center",
    "anchor_to": "center",
    "size": [ 90, 140 ],
    "controls": [
      {
        "skin_renderer_panel":{
          "type": "panel",
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "offset": [ 0, -14 ],
          "size": [ "100%", 108 ],
          "controls": [
            {
              "skin_model": {
                "type": "custom",
                "size": [ "100%", "100%" ],
                "renderer": "paper_doll_renderer",
                "layer": 4,
                "$skin_property_bag|default": 1,
                "use_selected_skin": false,
                "property_bag": {
                  "#preview_idx": "$skin_property_bag"
                },
                "animation_reset_name": "screen_animation_reset",
                "anims": [
                  "@common.screen_exit_size_animation_push",
                  "@common.screen_exit_size_animation_pop",
                  "@common.screen_entrance_size_animation_push",
                  "@common.screen_entrance_size_animation_pop"
                ],
                "$preset_gesture_source_control_name": "main_preset_persona",
                "$preset_appearance_binding_name": "#appearance_preset_preview",
                "bindings": [
                  {
                    "binding_type": "global",
                    "binding_name": "$preset_appearance_binding_name",
                    "binding_condition": "always"
                  },
                  {
                    "binding_type": "view",
                    "source_control_name": "$preset_gesture_source_control_name",
                    "source_property_name": "#gesture_delta_source",
                    "target_property_name": "#gesture_delta_source"
                  },
                  {
                    "binding_type": "view",
                    "source_control_name": "$preset_gesture_source_control_name",
                    "source_property_name": "#gesture_mouse_delta_x",
                    "target_property_name": "#gesture_mouse_delta_x"
                  },
                  {
                    "binding_name": "#is_preset_visible",
                    "binding_name_override": "#visible",
                    "binding_condition": "always"
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "add_button@common.button": {
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "offset": [ 0, 2.5 ],
          "size": [ 72, 108 ],
          "$pressed_button_name": "#appearance_preset_preview",
          "property_bag": {
            "#preview_idx": "$skin_property_bag"
          },
          "bindings": [
            {
              "binding_name": "#is_preset_add_visible",
              "binding_name_override": "#visible",
              "binding_condition": "always"
            }
          ],
          "controls": [
            {
              "hover@main_profile_dx.bg_image_base": {
                "color": [ 0.21, 0.21, 0.21 ],
                "layer": 1,
                "size": [ "100%", "100%" ]
              }
            },
            {
              "add_icon": {
                "type": "image",
                "texture": "textures/uidx/profile_screen/add_new_character_icon",
                "anchor_from": "top_middle",
                "anchor_to": "top_middle",
                "offset": [ 0, 38 ],
                "layer": 5,
                "size": [ 18, 18 ]
              }
            },
            { 
              "base_label@main_profile_dx.base_label": {
                "text": "controller.buttonTip.new",
