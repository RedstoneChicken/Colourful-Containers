  /*//// Base Textures ////*/

  "bg_image_base": {
    "type": "image",
    "texture": "textures/uidx/global/color_base"
  },

  "base_label": {
    "type": "label",
    "text": "$base_text",
    "size": [ "default", 10 ],
    "font_type": "smooth",
    "$base_text|default": "#edit_appearance_text",
    "bindings": "$base_text_bindings",
    "font_scale_factor": "$font_scale_factor",
    "$base_text_bindings|default": [
      {
        "binding_name": "$base_text"
      }
    ]
  },

  "left_base_button@common.button": {
    "size": [ "100%", 28 ],
    "$label_control": "main_profile_dx.base_label",
    "$font_scale_factor|default": 0.9,
    "$label_control|default": "main_profile_dx.base_label",
    "controls": [
      { 
        "base_label@$label_control": {
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "offset": [ 28, 9 ],
          "layer": 2,
          "font_scale_factor": "$font_scale_factor"
