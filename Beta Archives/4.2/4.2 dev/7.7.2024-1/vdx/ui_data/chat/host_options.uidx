        "return_bg": {
          "layer": 1,
          "type": "image",
          "size": [ "100%", 40 ],
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "offset": [ 20, 0 ],
          "texture": "vdx/ui_textures/chat/autocomplete_bg"
