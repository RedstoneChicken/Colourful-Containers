                  },
                  {
                    "subtext@label_dx.global_label": {
                      "shadow": false,
                      "text_alignment": "left",
                      "size": [ "default", 10 ],
                      "anchor_from": "top_left",
                      "anchor_to": "top_left",
                      "offset": [ 0, 11 ],
                      "text": "#toast_subtitle",
