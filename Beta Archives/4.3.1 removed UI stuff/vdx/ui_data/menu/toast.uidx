  /*//// Third Party Achievement Panels ////*/

  "third_party_achievement_panel": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%cm", 20 ],
    "controls": [
      {
        "icon": {
          "type": "panel",
          "size": [ 24, 20 ],
          "controls": [
            { "image@toast_desktop_dx.trophy_icon": { } }
          ]
        }
      },
      {
        "text_panel": {
          "type": "panel",
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "size": [ "100%cm+24px", 20 ],
          "layer": 4,
          "controls": [
            {
              "main_title@label_dx.global_label": {
                "color": [ 1.0, 1.0, 0.0 ],
                "shadow": false,
                "text_alignment": "left",
                "size": [ "default", 10 ],
                "anchor_from": "top_left",
                "anchor_to": "top_left",
                "offset": [ 0, 5 ],
                "text": "achievement.get",
                "bindings": [
                  {
                    "binding_name": "(not #toast_subtitle_visible)",
                    "binding_name_override": "#visible"
                  }
                ]
              }
            },
            {
              "title@label_dx.global_label": {
                "color": [ 1.0, 1.0, 0.0 ],
                "shadow": false,
                "text_alignment": "left",
                "size": [ "default", 10 ],
                "anchor_from": "top_left",
                "anchor_to": "top_left",
                "text": "achievement.get",
                "bindings": [
                  {
                    "binding_name": "(#toast_subtitle_visible)",
                    "binding_name_override": "#visible"
                  }
                ]
              }
            },
            {
              "subtitle": {
                "type": "stack_panel",
                "orientation": "horizontal",
                "anchor_from": "top_left",
                "anchor_to": "top_left",
                "offset": [ 0, 11 ],
                "size": [ "100%c", 10 ],
                "controls": [
                  {
                    "subtext_offset@common.empty_panel": {
                      "size": [ "100%c", 10 ],
                      "controls": [
                        {
                          "subtext@label_dx.global_label": {
                            "shadow": false,
                            "text_alignment": "left",
                            "size": [ "default", 10 ],
                            "anchor_from": "top_left",
                            "anchor_to": "top_left",
                            "text": "#toast_title",
                            "bindings": [
                              {
                                "binding_name": "#toast_title"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

