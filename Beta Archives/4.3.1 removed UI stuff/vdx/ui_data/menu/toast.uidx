  /*//// Recipe Unlocked Desktop ////*/

  "recipe_unlocked_desktop_popup@toast_desktop_dx.main_popup": {
    "size": [ 160, 32 ],
    "anchor_from": "top_right",
    "anchor_to": "bottom_right",
    "controls": [
      {
        "input_panel": {
          "type": "input_panel",
          "size": [ 160, 32 ],
          "offset": [ 0, -2 ],
          "anchor_from": "center",
          "anchor_to": "center",
          "gesture_tracking_button": "button.toast_interaction",
          "button_mappings": [
            {
              "from_button_id": "button.menu_select",
              "to_button_id": "button.toast_interaction",
              "mapping_type": "pressed",
              "button_up_right_of_first_refusal": true
            }
          ],
          "controls": [
            { "background@toast_textures_dx.recipe": { "layer": 1 } },
            {
              "item_renderer_type@toast_screen.item_renderer": {
                "layer": 4,
                "anchor_from": "top_left",
                "anchor_to": "top_left",
                "offset": [ 2, 2 ],
                "size": [ 9, 9 ],
                "property_bag": {
                  "#item_id_aux": 3801088
                }
              }
            },
            {
              "item_renderer@toast_screen.item_renderer": {
                "layer": 3,
                "anchor_from": "top_left",
                "anchor_to": "top_left",
                "offset": [ 8, 8 ],
                "size": [ 16, 16 ],
                "property_bag": {
                  "#item_id_aux": 3801088
                },
                "bindings": [
                  {
                    "binding_name": "#recipe_unlocked",
                    "binding_name_override": "#item_id_aux"
                  }
                ]
              }
            },
            {
              "title@label_dx.global_label": {
                "shadow": false,
                "text_alignment": "left",
                "color": [ 0.3, 0.0, 0.3 ],
                "text": "recipe.toast.title",
                "size": [ 125, 10 ],
                "layer": 2,
                "anchor_from": "top_left",
                "anchor_to": "top_left",
                "offset": [ 30, 6 ]
              }
            },
            {
              "description@label_dx.global_label": {
                "shadow": false,
                "text_alignment": "left",
                "color": [ 0.0, 0.0, 0.0 ],
                "text": "recipe.toast.description",
                "size": [ 125, 10 ],
                "offset": [ 30, 17 ],
                "layer": 2,
                "anchor_from": "top_left",
                "anchor_to": "top_left"
              }
            }
          ]
        }
      }
    ]
  },
  
