{
  "namespace": "blast_furnace_pocket_dx",

  /*//// Icons ////*/

  "blast_furnace_icon@container_panel_pocket_dx.item_renderer": { "$object_item_id": -196 }, //  Blast Furnace- ID: -196

  "left_navigation_tabs": {
    "type": "stack_panel",
    "size": [ 32, "100%" ],
    "anchor_from": "top_right",
    "anchor_to": "top_right",
    "offset": [ 0, 0 ],
    "controls": [
      { "fill_panel": { "type": "panel", "size": [ 1, "fill" ] } },
      {
        "inventory_tab_panel": {
          "type": "panel",
          "size": [ 32, 28 ],
          "controls": [
            { 
              "inventory_tab@container_panel_pocket_dx.decorated_tab_panel_template": {
                "$tab_selected_texture": "inventory_pocket_textures_dx.tab_left_selected_1",
                "$tab_icon": "recipe_book_pocket_navigation_tabs_dx.survival_inventory_icon"
              }
            }
          ]
        }
      }
    ]
  },

  "right_navigation_tabs": {
    "type": "stack_panel",
    "size": [ 32, "100%" ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "offset": [ 0, 0 ],
    "controls": [
      { "fill_panel": { "type": "panel", "size": [ 1, "fill" ] } },
      {
        "blast_furnace_tab_panel": {
          "type": "panel",
          "size": [ 32, 28 ],
          "controls": [
            { 
              "search_tab@container_panel_pocket_dx.decorated_tab_panel_template": {
                "$tab_anchor_side": "top_right",
                "$tab_selected_texture": "blast_furnace_pocket_textures_dx.tab_right_selected_1",
                "$tab_icon": "blast_furnace_pocket_dx.blast_furnace_icon"
              }
            }
          ]
        }
      }
    ]
  },
  
  /*//// Inventory Panel ////*/

  "invnetory_left_panel": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      { "background@inventory_pocket_textures_dx.inventory_bg_half": { "layer": 1 } },
      { "content@container_panel_pocket_dx.pocket_container_panel_template": { "layer": 10 } }
    ]
  },
  
  /*//// Blast Furnace Panel ////*/

  "blast_furnace_right_panel": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      { "furnace_bg@blast_furnace_pocket_textures_dx.blast_furnace_bg": { "layer": 1 } },
      /* Title */
      {
        "title@label_dx.global_label": {
          "layer": 10,
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "text_alignment": "center",
          "offset": [ 0, 10 ],
          "color": "$vdx_blast_furnace_pocket_title_text_color",
          "text": "$container_title",
          "shadow": "$vdx_container_text_shadow",
          "controls": [
            { "text_control@$vdx_blast_furnace_pocket_title_text_control": { } }
          ]
        }
      },
      {
        "center_hold": {
          "type": "panel",
          "anchor_to": "top_left",
          "anchor_from": "top_left",
          "offset": [ 7, 27 ],
          "size": [ "100%-14px", "100%-34px" ],
          "controls": [
            {
              "main_blast_furnace_panel": {
                "type": "panel",
                "size": [ 114, 94 ],
                "controls": [
                  { "bg@blast_furnace_pocket_textures_dx.blast_furnace_panel": { "layer": 1 } },
                  /* Ingredient and Fuel */
                  {
                    "ingredient_panel": {
                      "type": "panel",
                      "layer": 10,
                      "size": [ 28, 28 ],
                      "anchor_from": "top_left",
                      "anchor_to": "top_left",
                      "offset": [ 7, 7 ],
                      "controls": [
                        {
                          "text@label_dx.global_label": {
                            "layer": 10,
                            "anchor_from": "top_middle",
                            "anchor_to": "top_middle",
                            "text_alignment": "center",
                            "offset": [ 0, -15 ],
                            "color": "$vdx_blast_furnace_pocket_input_text_color",
                            "text": "furnaceScreen.input",
                            "shadow": "$vdx_container_text_shadow",
                            "controls": [
                              { "text_control@$vdx_blast_furnace_pocket_input_text_control": { } }
                            ]
                          }
                        },
                        { 
                          "blast_furnace_ingredient_item@container_item_dx.container_item": {
                            "size": [ 28, 28 ],
                            "$cell_image_size": [ 28, 28 ],
                            "$lock_cell_slot_control": "pocket_textures_dx.lock_cell_slot",
                            "$slot_highlight_image": "pocket_textures_dx.slot_highlight",
                            "$item_collection_name": "furnace_ingredient_items",
                            "$focus_id": "furnace_ingredient_item",
                            "$focus_override_left": "FOCUS_OVERRIDE_STOP",
                            "$focus_override_left_binding_type": "none",
                            "$focus_override_up": "FOCUS_OVERRIDE_STOP",
                            "$focus_override_up_binding_type": "none"
                          }
                        }
                      ]
                    }
                  },
                  { 
                    "lit_progress@blast_furnace_pocket_textures_dx.lit_progress": {
                      "layer": 10,
                      "anchor_to": "top_left",
                      "anchor_from": "top_left",
                      "offset": [ 13, 40 ]
                    }
                  },
                  {
                    "fuel_panel": {
                      "type": "panel",
                      "layer": 10,
                      "size": [ 28, 28 ],
                      "anchor_from": "top_left",
                      "anchor_to": "top_left",
                      "offset": [ 7, 59 ],
                      "controls": [
                        {
                          "text@label_dx.global_label": {
                            "layer": 10,
                            "anchor_from": "top_middle",
                            "anchor_to": "top_middle",
                            "text_alignment": "center",
                            "offset": [ 0, 33 ],
                            "color": "$vdx_blast_furnace_pocket_fuel_text_color",
                            "text": "furnaceScreen.fuel",
                            "shadow": "$vdx_container_text_shadow",
                            "controls": [
                              { "text_control@$vdx_blast_furnace_pocket_fuel_text_control": { } }
                            ]
                          }
                        },
                        { 
                          "blast_furnace_fuel_item@container_item_dx.container_item": {
                            "size": [ 28, 28 ],
                            "$cell_image_size": [ 28, 28 ],
                            "$lock_cell_slot_control": "pocket_textures_dx.lock_cell_slot",
                            "$slot_highlight_image": "pocket_textures_dx.slot_highlight",
                            "$item_collection_name": "furnace_fuel_items",
                            "$focus_id": "furnace_fuel_item",
                            "$focus_override_right": "furnace_output_item",
                            "$focus_override_right_binding_type": "none",
                            "$focus_override_left": "FOCUS_OVERRIDE_STOP",
                            "$focus_override_left_binding_type": "none"
                          }
                        }
                      ]
                    }
                  },
                  /* Output */
                  { 
                    "burn_progress@blast_furnace_pocket_textures_dx.burn_progress": {
                      "layer": 10,
                      "anchor_to": "top_left",
                      "anchor_from": "top_left",
                      "offset": [ 44, 39 ]
                    }
                  },
                  {
                    "output_panel": {
                      "type": "panel",
                      "layer": 10,
                      "size": [ 28, 28 ],
                      "anchor_to": "top_left",
                      "anchor_from": "top_left",
                      "offset": [ 79, 33 ],
                      "controls": [
                        {
                          "text@label_dx.global_label": {
                            "layer": 10,
                            "anchor_from": "top_middle",
                            "anchor_to": "top_middle",
                            "text_alignment": "center",
                            "offset": [ 0, -15 ],
                            "color": "$vdx_blast_furnace_pocket_result_text_color",
                            "text": "furnaceScreen.result",
                            "shadow": "$vdx_container_text_shadow",
                            "controls": [
                              { "text_control@$vdx_blast_furnace_pocket_result_text_control": { } }
                            ]
                          }
                        },
                        {
                          "result_item@label_dx.global_label": {
                            "layer": 10,
                            "anchor_from": "top_middle",
                            "anchor_to": "top_middle",
                            "text_alignment": "center",
                            "offset": [ 0, 33 ],
                            "color": "$vdx_blast_furnace_pocket_item_text_color",
                            "text": "#text",
                            "bindings": [
                              {
                                "binding_name": "#output_name",
                                "binding_name_override": "#text"
                              }
                            ],
                            "shadow": "$vdx_container_text_shadow",
                            "controls": [
                              { "text_control@$vdx_blast_furnace_pocket_item_text_control": { } }
                            ]
                          }
                        },
                        { 
                          "blast_furnace_output_item@container_item_dx.container_item": {
                            "size": [ 28, 28 ],
                            "$cell_image_size": [ 28, 28 ],
                            "$lock_cell_slot_control": "pocket_textures_dx.lock_cell_slot",
                            "$slot_highlight_image": "pocket_textures_dx.slot_highlight",
                            "$item_collection_name": "furnace_output_items",
                            "$focus_id": "furnace_output_item",
                            "$focus_override_right_binding_type": "none",
                            "$focus_override_right": "FOCUS_OVERRIDE_STOP",
                            "$focus_override_left_binding_type": "none",
                            "$focus_override_left": "furnace_fuel_item"
                          }
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },
  
  /*//// Main Panel ////*/

  "blast_furnace_panel@container_panel_pocket_dx.double_container_panel": {
    "size": [ "100%", "100%-4px" ],
    "$left_tab_control": "blast_furnace_pocket_dx.left_navigation_tabs",
    "$left_panel_control": "blast_furnace_pocket_dx.invnetory_left_panel",
    "$center_panel_control": "blast_furnace_pocket_textures_dx.center_fold",
    "$right_panel_control": "blast_furnace_pocket_dx.blast_furnace_right_panel",
    "$right_tab_control": "blast_furnace_pocket_dx.right_navigation_tabs"
  },
  
  /*//// Screen ////*/

  "main_screen_content": {
    "type": "panel",
    "controls": [
      { "container_modules@container_panel_pocket_dx.container_modules": {} },
      { "furnace_panel@blast_furnace_pocket_dx.blast_furnace_panel": {} }
    ]
  },

  "center_screen": {
    "type": "panel",
    "controls": [
      { "main_screen_content@blast_furnace_pocket_dx.main_screen_content": { } }
    ]
  },

  "full_screen": {
    "type": "panel",
    "controls": [
      { "screen_background@main_textures_dx.screen_background": { "layer": -10 } }
    ]
  }
}