{
  "namespace": "chest_large_pocket_dx",
  
  /*//// Tabs ////*/

  "chest_icon@container_panel_pocket_dx.item_renderer": { "$object_item_id": 54 }, //  Chest Small - ID: 54

  "left_navigation_tabs": {
    "type": "stack_panel",
    "size": [ 32, "100%" ],
    "anchor_from": "top_right",
    "anchor_to": "top_right",
    "offset": [ 0, 0 ],
    "controls": [
      { "fill_panel": { "type": "panel", "size": [ 1, "fill" ] } },
      {
        "inventory_tab_panel": {
          "type": "panel",
          "size": [ 32, 28 ],
          "controls": [
            { 
              "inventory_tab@container_panel_pocket_dx.decorated_tab_panel_template": {
                "$tab_selected_texture": "inventory_pocket_textures_dx.tab_left_selected_1",
                "$tab_icon": "recipe_book_pocket_navigation_tabs_dx.survival_inventory_icon"
              }
            }
          ]
        }
      }
    ]
  },

  "right_navigation_tabs": {
    "type": "stack_panel",
    "size": [ 32, "100%" ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "offset": [ 0, 0 ],
    "controls": [
      { "fill_panel": { "type": "panel", "size": [ 1, "fill" ] } },
      {
        "chest_large_tab_panel": {
          "type": "panel",
          "size": [ 32, 28 ],
          "controls": [
            { 
              "search_tab@container_panel_pocket_dx.decorated_tab_panel_template": {
                "$tab_anchor_side": "top_right",
                "$tab_selected_texture": "chest_pocket_textures_dx.tab_right_selected_1",
                "$tab_icon": "chest_large_pocket_dx.chest_icon"
              }
            }
          ]
        }
      }
    ]
  },
  
  /*//// Inventory Panel ////*/

  "invnetory_left_panel": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      { "background@inventory_pocket_textures_dx.inventory_bg_half": { "layer": 1 } },
      { "content@container_panel_pocket_dx.pocket_container_panel_template": { "layer": 10 } }
    ]
  },
  
  /*//// chest_large Panel ////*/

  "chest_large_right_panel": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      { "chest_large_bg@chest_pocket_textures_dx.chest_bg": { "layer": 1 } },
      { 
        "content@container_panel_pocket_dx.pocket_container_panel_template": {
          "$scroll_box_image_control": "chest_pocket_textures_dx.scroller",
          "$scroll_track_image_control": "chest_pocket_textures_dx.scroll_track",
          "$scroll_background_image_control": "chest_pocket_textures_dx.chest_content_bg",
          "$scrolling_content": "chest_large_pocket_dx.chest_large_grid",
          "$pane_collection": "container_items",
          "$container_size":54,
          "layer": 10,
          "controls": [
            {
              "title_label@label_dx.global_label": {
                "size": [ "default", 10 ],
                "anchor_from": "top_middle",
                "anchor_to": "top_middle",
                "text_alignment": "center",
                "offset": [ 0, 10 ],
                "color": "$vdx_chest_pocket_title_text_color",
                "text": "$container_title",
                "layer": 3,
                "shadow": "$vdx_container_text_shadow",
                "controls": [
                  { "text_control@$vdx_chest_pocket_title_text_control": { } }
                ]
              }
            },
            {
              "main@scroll_dx.contained_scrolling_panel": {
                "anchor_to": "top_left",
                "anchor_from": "top_left",
                "offset": [ 7, 27 ],
                "size": [ "100%-14px", "100%-34px" ],
                "$scroll_size": [ 14, "100%" ],
                "$scroll_view_port_size": [ "100%-2px", "100%-2px" ],
                "$scroll_view_port_offset": [ 1, 1 ],
                "$scrolling_pane_size": [ "100%", "100%" ]
              }
            }
          ]
        }
      }
    ]
  },

  "chest_large_grid": {
    "type": "panel",
    "size": [ "100%", "100%c" ],
    "controls": [
      {
        "grid": {
          "type": "grid",
          "grid_rescaling_type": "horizontal",
          "size": [ "100%", "default" ],
          "anchor_to": "top_left",
          "anchor_from": "top_left",
          "maximum_grid_items": "$container_size",
          "collection_name": "$pane_collection",
          "grid_item_template": "chest_large_pocket_dx.chest_large_container_item",
          "$item_collection_name": "$pane_collection"
        }
      }
    ]
  },

  "chest_large_container_item@container_item_dx.container_item": {
    "$cell_image_size": [ 28, 28 ],
    "$lock_cell_slot_control": "pocket_textures_dx.lock_cell_slot",
    "$slot_highlight_image": "pocket_textures_dx.slot_highlight",
    "$background_images": "chest_pocket_textures_dx.chest_grid",
    "size": [ 28, 28 ]
  },
  
  /*//// Main Panel ////*/

  "chest_large_panel@container_panel_pocket_dx.double_container_panel": {
    "size": [ "100%", "100%-4px" ],
    "$left_tab_control": "chest_large_pocket_dx.left_navigation_tabs",
    "$left_panel_control": "chest_large_pocket_dx.invnetory_left_panel",
    "$center_panel_control": "chest_pocket_textures_dx.center_fold",
    "$right_panel_control": "chest_large_pocket_dx.chest_large_right_panel",
    "$right_tab_control": "chest_large_pocket_dx.right_navigation_tabs"
  },
  
  /*//// Screen ////*/

  "main_screen_content": {
    "type": "panel",
    "controls": [
      { "container_modules@container_panel_pocket_dx.container_modules": {} },
      { "chest_large_panel@chest_large_pocket_dx.chest_large_panel": {} }
    ]
  },

  "center_screen": {
    "type": "panel",
    "controls": [
      { "main_screen_content@chest_large_pocket_dx.main_screen_content": { } }
    ]
  },

  "full_screen": {
    "type": "panel",
    "controls": [
      { "screen_background@main_textures_dx.screen_background": { "layer": -10 } }
    ]
  }
}