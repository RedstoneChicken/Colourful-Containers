  "recipe_book_title_label": {
    "type": "panel",
    "controls": [
      {
        "half_panel": {
          "type": "panel",
          "bindings": [
            {
              "binding_type": "global",
              "binding_name": "(not #is_creative_layout)",
              "binding_name_override": "#visible"
            }
          ],
          "controls": [
            {
              "recipe_tab_label@label_dx.global_label": {
                "text": "$tab_label_text",
                "size": [ "default", 10 ],
                "anchor_from": "top_left",
                "anchor_to": "top_left",
                "text_alignment": "left",
                "offset": [ 8, 10 ],
                "color": "$vdx_recipe_book_items_pocket_title_text_color",
                "layer": 3,
                "shadow": "$vdx_container_text_shadow",
                "controls": [
                  { "text_control@$vdx_recipe_book_items_pocket_title_text_control": { } }
                ],
                "bindings": [
                  {
                    "binding_type": "global",
                    "binding_name": "(not #is_creative_mode)",
                    "binding_name_override": "#visible"
                  }
                ]
              }
            },
            {
              "creative_tab_label@label_dx.global_label": {
                "text": "$tab_label_text",
                "size": [ "default", 10 ],
                "anchor_from": "top_middle",
                "anchor_to": "top_middle",
                "text_alignment": "center",
                "offset": [ 0, 10 ],
                "color": "$vdx_creative_items_pocket_title_text_color",
                "layer": 3,
                "shadow": "$vdx_container_text_shadow",
                "controls": [
                  { "text_control@$vdx_creative_items_pocket_title_text_control": { } }
                ],
                "bindings": [
                  {
                    "binding_type": "global",
                    "binding_name": "(#is_creative_mode)",
                    "binding_name_override": "#visible"
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "full_creative_tab_label@label_dx.global_label": {
          "text": "$tab_label_text",
          "size": [ "default", 10 ],
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "text_alignment": "center",
          "offset": [ 0, 10 ],
          "color": "$vdx_full_creative_items_pocket_title_text_color",
          "layer": 3,
          "shadow": "$vdx_container_text_shadow",
