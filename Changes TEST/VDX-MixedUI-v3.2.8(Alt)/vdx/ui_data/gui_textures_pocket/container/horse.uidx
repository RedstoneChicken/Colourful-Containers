  "equip_panel": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "background": {
          "type": "image",
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/horse/equip_panel",
          "size": [ "100%", "100%" ]
        }
      }
    ]
  },

  "equip_slot": {
    "type": "panel",
    "size": [ 28, 28 ],
    "controls": [
      {
