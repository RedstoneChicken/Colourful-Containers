{
  "namespace": "horse_pocket_textures_dx",

  "equip_bg": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "background": {
          "type": "image",
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/horse/equip_bg",
          "size": [ "100%", "100%" ]
        }
      }
    ]
  },

  "chest_bg": {
