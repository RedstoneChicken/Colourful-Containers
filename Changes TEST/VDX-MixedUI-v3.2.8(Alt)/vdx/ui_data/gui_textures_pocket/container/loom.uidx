{
  "namespace": "loom_pocket_textures_dx",

  "loom_bg": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "background": {
          "type": "image",
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/loom/loom_bg",
          "size": [ "100%", "100%" ]
        }
      }
    ]
  },

  "patterns_bg": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "background": {
          "type": "image",
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/loom/patterns_bg",
          "size": [ "100%", "100%" ]
        }
      }
    ]
  },

  "loom_panel": {
    "type": "panel",
    "size": [ 164, 106 ],
    "controls": [
      {
        "background": {
          "type": "image",
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/loom/loom_panel",
          "size": [ "100%", "100%" ]
        }
      }
    ]
  },

  "pattern": {
    "type": "panel",
    "size": [ 28, 28 ],
    "controls": [
      {
        "texture": {
          "type": "image",
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/loom/pattern",
          "size": [ "100%", "100%" ]
        }
      }
    ]
  },

  "pattern_highlighted": {
    "type": "panel",
    "size": [ 28, 28 ],
    "controls": [
      {
        "texture": {
          "type": "image",
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/loom/pattern_highlighted",
          "size": [ "100%", "100%" ]
        }
      }
    ]
  },

  "pattern_selected": {
    "type": "panel",
    "size": [ 28, 28 ],
    "controls": [
      {
        "texture": {
          "type": "image",
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/loom/pattern_selected",
          "size": [ "100%", "100%" ]
        }
      }
    ]
  },

  /* Scroller */

  "scroller": {
    "type": "panel",
    "size": [ 12, "100%" ],
    "controls": [
      {
        "texture": {
          "type": "image",
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/loom/scroller",
          "size": [ 12, "100%-2px" ],
          "tiled": true, // Only if the texture is nineslice type.
          "tiled_scale": "$sprite_tiled_scale",
          "variables": [
            { "requires": "($loom_pocket_scroller_resolution = '1x')",  "$sprite_tiled_scale": [ 1, 1 ] },
            { "requires": "($loom_pocket_scroller_resolution = '2x')",  "$sprite_tiled_scale": [ 0.5, 0.5 ] },
            { "requires": "($loom_pocket_scroller_resolution = '4x')",  "$sprite_tiled_scale": [ 0.25, 0.25 ] },
            { "requires": "($loom_pocket_scroller_resolution = '8x')",  "$sprite_tiled_scale": [ 0.125, 0.125 ] },
            { "requires": "($loom_pocket_scroller_resolution = '16x')", "$sprite_tiled_scale": [ 0.0625, 0.0625 ] },
            { "requires": "($loom_pocket_scroller_resolution = '32x')", "$sprite_tiled_scale": [ 0.03125, 0.03125 ] }
          ]
        }
      }
    ]
  },

  "scroll_track": {
    "type": "panel",
    "size": [ 14, "100%" ],
    "controls": [
      {
        "texture": {
          "type": "image",
          "size": [ 14, "100%" ],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "offset": [ 0, 0 ],
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/loom/scroll_track"
        }
      }
    ]
  },

  "patterns_content_bg": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "texture": {
          "type": "image",
          "size": [ "100%", "100%" ],
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/loom/patterns_content_bg"
        }
      }
    ]
  },

  /* Center Fold */

  "center_fold": {
    "type": "panel",
    "size": [ 18, "100%" ],
    "controls": [
      {
        "texture": {
          "type": "image",
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/loom/center_fold",
          "size": [ "100%", "100%" ]
        }
      }
    ]
  },

  /* Loom Tab */

  "tab_right_selected_1": {
    "type": "panel",
    "size": [ 40, 28 ],
    "controls": [
      {
        "texture": {
          "type": "image",
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/loom/tab_right_selected_1",
          "size": [ "100%", "100%" ]
        }
      }
    ]
  },

  /* Pattern Tab */

  "tab_left_selected_1": {
    "type": "panel",
    "size": [ 40, 28 ],
    "controls": [
      {
        "texture": {
          "type": "image",
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/loom/tab_left_selected_1",
          "size": [ "100%", "100%" ]
        }
      }
    ]
  },

  "tab_left_unselected_1": {
    "type": "panel",
    "size": [ 40, 28 ],
    "controls": [
      {
        "texture": {
          "type": "image",
          "texture": "assets/vdx/textures/gui/sprites/container_pocket/loom/tab_left_unselected_1",
          "size": [ "100%", "100%" ]
        }
      }
    ]
  }
}