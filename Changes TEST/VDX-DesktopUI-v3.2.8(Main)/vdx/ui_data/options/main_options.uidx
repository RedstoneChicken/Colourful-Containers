        }
      },
      {
        "controls_button@options_dx.button_tab": {
          "size": [ 150, 20 ],
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "offset": [ 0, 24 ],
          "$button_text": "options.controls",
          "$tab_view_binding_name": "controls_button_toggle",
          "$toggle_group_forced_index": 107
        }
      },
      {
        "more_button@options_dx.button_tab": {
          "size": [ 150, 20 ],
          "anchor_from": "top_right",
