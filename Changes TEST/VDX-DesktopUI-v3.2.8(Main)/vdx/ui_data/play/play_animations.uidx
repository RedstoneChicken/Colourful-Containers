              "binding_collection_name": "$collection_name"
            }
          ]
        }
      },
      { 
        "ping_offline@server_list_textures_dx.unreachable": {
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "bindings": [
            {
              "binding_name": "(#texture_name = 'textures/ui/Ping_Offline_Red')",
