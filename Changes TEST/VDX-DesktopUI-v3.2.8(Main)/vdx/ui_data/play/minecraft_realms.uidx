  /*//// Realms World Item ////*/
  
  "status_button_base@common.button": {
    "size": [ 10, 28 ],
    "layer": 8,
    "controls": [
      {
        "hover": {
          "type": "panel",
          "size": [ 10, 28 ],
          "$hover_text_texture": "vdx/ui_textures/hover_text/text_panel",
          "controls": [
            {
              "hover_text_panel@tooltip_dx.tooltip_panel": {
                "anchor_from": "top_right",
                "anchor_to": "top_right"
              }
            }
          ]
        }
      }
    ]
  },

  "realms_world_item": {
    "type": "panel",
    "size": [ "100%", 36 ],
    "controls": [
      {
        "status_hold": {
          "type": "panel",
          "size": [ 10, 28 ],
          "anchor_from": "top_right",
