  /*//// Realms Status Hover Text ////*/
  
  "on_tooltip_label@tooltip_dx.tooltip_label": {
    "size": [ "default", 10 ],
    "text": "options.on"
  },

  "closed_tooltip_label@tooltip_dx.tooltip_label": {
    "size": [ "default", 10 ],
    "text": "options.off"
  },
  
  "expired_tooltip_label@tooltip_dx.tooltip_label": {
    "size": [ "default", 10 ],
