{
  "namespace": "widget_slider_textures_dx",

  /* Slider 310 */

  "slider_310": {
    "type": "panel",
    "size": [ 310, 20 ],
    "controls": [
      {
        "texture": {
          "type": "image",
          "tiled": true, // Only if the texture is nineslice type.
          "tiled_scale": "$sprite_tiled_scale",
          "texture": "assets/minecraft/textures/gui/sprites/widget/slider",
          "size": [ "100%", "100%" ],
          "variables": [
            { "requires": "($widget_slider_resolution = '1x')",  "$sprite_tiled_scale": [ 1, 1 ] },
            { "requires": "($widget_slider_resolution = '2x')",  "$sprite_tiled_scale": [ 0.5, 0.5 ] },
            { "requires": "($widget_slider_resolution = '4x')",  "$sprite_tiled_scale": [ 0.25, 0.25 ] },
            { "requires": "($widget_slider_resolution = '8x')",  "$sprite_tiled_scale": [ 0.125, 0.125 ] },
            { "requires": "($widget_slider_resolution = '16x')", "$sprite_tiled_scale": [ 0.0625, 0.0625 ] },
            { "requires": "($widget_slider_resolution = '32x')", "$sprite_tiled_scale": [ 0.03125, 0.03125 ] }
          ]
        }
      }
    ]
  },

