{
  "namespace": "popup_textures_dx",

  "background": {
    "type": "panel",
    "controls": [
      {
        "texture": {
          "type": "image",
          "tiled": true, // Only iF the texture is nineslice type.
          "tiled_scale": "$sprite_tiled_scale",
          "texture": "assets/minecraft/textures/gui/sprites/popup/background",
          "size": [ "100%", "100%" ],
          "variables": [
            { "requires": "($popup_background_resolution = '1x')",  "$sprite_tiled_scale": [ 1, 1 ] },
            { "requires": "($popup_background_resolution = '2x')",  "$sprite_tiled_scale": [ 0.5, 0.5 ] },
            { "requires": "($popup_background_resolution = '4x')",  "$sprite_tiled_scale": [ 0.25, 0.25 ] },
            { "requires": "($popup_background_resolution = '8x')",  "$sprite_tiled_scale": [ 0.125, 0.125 ] },
            { "requires": "($popup_background_resolution = '16x')", "$sprite_tiled_scale": [ 0.0625, 0.0625 ] },
            { "requires": "($popup_background_resolution = '32x')", "$sprite_tiled_scale": [ 0.03125, 0.03125 ] }
          ]
        }
      }
    ]
  }
}