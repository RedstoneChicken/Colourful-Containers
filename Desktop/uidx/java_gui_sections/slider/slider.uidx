      { "requires": "($slider_resolution = '512x')",  "$slider_uv": [ 392,  0 ], "$slider_uv_size": [ 8, 40 ] },
      { "requires": "($slider_resolution = '1024x')", "$slider_uv": [ 784,  0 ], "$slider_uv_size": [ 16, 80 ] },
      { "requires": "($slider_resolution = '2048x')", "$slider_uv": [ 1568, 0 ], "$slider_uv_size": [ 32, 160 ] },
      { "requires": "($slider_resolution = '4096x')", "$slider_uv": [ 3136, 0 ], "$slider_uv_size": [ 64, 320 ] },
      { "requires": "($slider_resolution = '8192x')", "$slider_uv": [ 6272, 0 ], "$slider_uv_size": [ 128, 640 ] }
    ]
