  "recipe_book_panel@common.root_panel": {
    "type": "input_panel",
    "size": [ 147, 166 ],
    "$is_blast_furnace|default": false,
    "$is_smoker|default": false,
    "controls": [
      {
        "panel_content": {
          "type": "panel",
