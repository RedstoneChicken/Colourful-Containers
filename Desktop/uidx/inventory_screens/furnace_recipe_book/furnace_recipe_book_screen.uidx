                  },
                  { "stone_tab@furnace_recipe_book_dx.stone_tab": { "ignored": "$is_smoker" } },
                  {
                    "fill_panel_2": {
                      "type": "panel",
                      "size": [ "fill", 1 ]
                    }
                  },
                  { "materials_tab@furnace_recipe_book_dx.materials_tab": { "ignored": "$is_smoker" } }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  /*//// Tabs Icons ////*/

  "tab_icon": {
    "type": "panel",
    "size": [ 32, 26 ],
