                "controls": [
                  {
                    "safe_zone_slider_y@slider_dx.option_slider_310": {
                      "size": [ 310, 20 ],
                      "layer": 10,
                      "anchor_from": "top_left",
                      "anchor_to": "top_left",
                      "$option_label": "#screen_position_y_slider_label",
                      "$slider_name": "screen_position_y",  
