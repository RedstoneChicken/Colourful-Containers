  "mob_effect_grid_panel": {
    "type": "panel",
    "size": [ "100%", "100%c" ],
    "controls": [
      {
        "mob_effect_grid@mob_effect.mob_effect_grid": {
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "size": [ "100%", "default" ],
          "offset": [ 0, 0 ],
          "grid_item_template": "mob_effect_dx.mob_effect_grid_item"
        }
      }
    ]
  },
  
  /*//// Screen ////*/
  
  "cross": {
    "type": "image",
    "layer": 3,
    "size": [ 16, 16 ],
    "texture": "vdx/ui_textures/icon/cross"
  },

  "center_screen": {
    "type": "panel",
    "controls": [
      {
        "close_button@button_dx.main_black_button": {
          "ignored": "(not $touch)",
          "$pressed_button_name": "button.menu_exit",
          "$button_content": "mob_effect_dx.cross_icon",
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "offset": [ -4, 4 ],
          "size": [ 16, 16 ],
          "bindings": [
            {
              "binding_name": "#close_button_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "scrolling_panel@scroll_dx.scrolling_panel": {
          "layer": 10,
          "size": [ 150, "100%" ],
          "$scrolling_pane_size": [ "100%", "100%" ],
          "$scroll_view_port_size": [ "100%", "100%" ],
          "$scroll_view_port_size_touch": [ "100%", "100%" ],
          "$scrolling_pane_offset": [ 0, 0 ],
          "$scroll_track_image_control": "main_textures_dx.scroll_track",
          "$scroll_box_image_control": "widget_textures_dx.scroller",
          "$scroll_size": [ 6, "100%" ],
          "$scrolling_content": "mob_effect_dx.mob_effect_grid_panel",
          "$jump_to_bottom_on_update": true,
          "$always_handle_scrolling": true
        }
      }
    ]
  },

  "full_screen": {
    "type": "panel",
    "controls": [
      { "screen_background@main_textures_dx.screen_background": { "layer": -10 } }
    ]
  }
}